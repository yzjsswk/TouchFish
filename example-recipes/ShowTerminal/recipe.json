{
    "bundleId": "com.yzjsswk.ShowTerminal",
    "version": 1,
    "author": "yzjsswk",
    "type": "task",
    "name": "Show Terminal",
    "command": ">",
    "icon": "system:apple.terminal.fill",
    "actions": [
        {
            "type": "shell",
            "arguments": [
                {
                    "type": "plain",
                    "value": "osascript"
