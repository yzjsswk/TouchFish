{
    "bundleId": "com.yzjsswk.SyncGoogleBookmark",
    "version": 1,
    "author": "yzjsswk",
    "type": "task",
    "name": "Sync Google Bookmark",
    "icon": "system:arrow.triangle.2.circlepath",
    "actions": [
        {
            "type": "shell",
            "arguments": [
                {
                    "type": "plain",
                    "value": "/usr/local/bin/python3"
                },
                {
                    "type": "file",
                    "value": "sync_google_bookmark.py"
                },
                {
                    "type": "context",
                    "value": "host"
                },
                {
                    "type": "context",
                    "value": "port"
                },
                {
                    "type": "context",
                    "value": "support_path"
                }
            ]
        },
        {
            "type": "back",
            "arguments": []
        }
    ]
}
